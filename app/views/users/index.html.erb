<dl class="sub-nav"  style="margin-bottom: 2px;">
	<dt>
		<%= t(:user_list)%>:
	</dt>
	<dd class="active" style="float: right" >
		<%= link_to t(:new ,:obj => t(:user)), new_user_path %>
	</dd>
</dl>
<table width="100%">
  <thead>
    <tr>
      <th><%= User.human_attribute_name :name %></th>
      <th><%= User.human_attribute_name :email %></th>
      <th><%= User.human_attribute_name :sign_in_count %></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.sign_in_count %></td>
        <td><%= link_to t(:show), user %></td>
        <td><%= link_to t(:edit), edit_user_path(user) %></td>
        <!--<td><%= link_to t(:destroy), user, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
